                "storage.type.js"
              ],
              "settings": {
                "fontStyle": "italic"
              }
            },
            {
              "scope": [
                "keyword.control.conditional",
                "keyword.operator"
              ],
              "settings": {
                "fontStyle": ""
              }
            }
          ]
        },
      
}